{
  "name": "@gdjiami/rc-components",
  "version": "0.1.2",
  "description": "Jiami React Components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf lib es",
    "build:es6": "tsc -d --outDir ./lib --module CommonJS ",
    "postbuild:es6": "cpx \"components/**/*.css\" ./lib",
    "postbuild:es6module": "cpx \"components/**/*.css\" ./es",
    "build:es6module": "tsc -d --outDir ./es",
    "build": "npm run build:es6 && npm run build:es6module",
    "lint": "tslint -c tslint.json \"components/**/*.ts?(x)\"",
    "prettier": "prettier --config .prettierrc --write \"components/**/*.ts?(x)\"",
    "precommit": "lint-staged",
    "parcel": "parcel"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "npm run prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@code.ejiahe.com:25522/WEB/jm-rc-components.git"
  },
  "files": [
    "lib",
    "es",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "react components"
  ],
  "author": "ivan",
  "license": "MIT",
  "peerDependencies": {
    "antd": "^3.6.6",
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.2.7",
    "antd": "^3.6.6",
    "cpx": "^1.5.0",
    "husky": "^0.14.3",
    "jh-tslint": "^1.0.0",
    "lint-staged": "^7.2.0",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-react": "3.2.0",
    "typescript": "^2.9.2",
    "react": "^16.0.0",
    "react-dom": "^16.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}